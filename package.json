{
  "name": "iTracker",
  "version": "0.0.1",
  "description": "CS498RK project server",
  "main": "src/server.js",
  "scripts": {
    "ins": "yarn install && cd client && yarn install && cd ..",
    "server": "nodemon --exec babel-node src/server.js",
    "build": "babel src --out-dir dist",
    "build-client": "cd client && yarn install && yarn build && cp -R build .. && cd .. && rm -rf public && mv build public",
    "start": "node dist/server.js",
    "client": "cd client && yarn start",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "git push heroku master"
  },
  "authors": [
    "Tongyu Wang (twang108)",
    "Yitong Song (ysong92)",
    "Zong Heng (hzong2)",
    "Siqi Gu (siqig2)",
    "Mingze Xiao (mxiao6)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mxiao6/CS498RK-project.git"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.0",
    "body-parser": "^1.17.2",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.2",
    "bcryptjs": "^2.4.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.4.18"
  }
}
